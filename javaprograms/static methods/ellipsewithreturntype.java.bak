class ellipse
	static double area()
	{
		double pi=3.142;
	    int a=4;
		int b=5;
		double ellipse=pi*a*b;
		return ellipse;
	}
}
class  ellipsewithreturntype
{
 public static void main(String[] args) 
	{
		double  ellipsearea= ellipse.area();
		System.out.println("the ellipse area is" +ellipsearea);
	}
}